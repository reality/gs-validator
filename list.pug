.container  
  - var c = 0;
  h1 Gold Standard Creator
  button#save.submit(value='Submit' style='font-size:20px') Save Results
  h3 Patients
  table#plist.stripe
    thead
      tr
        th(style='width:100%') ID
    tfoot
      tr
        th(style='width:18%') ID
    tbody
      each id in sample
        - c++
        tr
          -if(results[uid])
            td(style='text-align:left;background-color:green;')
              a(href='javascript:f.loadView("'+id+'")' style='color: white') #{id}
          -else
            td(style='text-align:left')
              a(href='javascript:f.loadView("'+id+'")') #{id}
